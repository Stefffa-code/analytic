{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "dashboard",
  "main": "app.js",
  "scripts": {
    "client": "npm run serve --prefix client",
    "client:build": "npm run build --prefix client",
    "client:install": "npm install --prefix client",
    "start": "NODE_ENV=production node app.ts &",
    "server": "tsc -sourcemap --module esnext --target esnext app.ts && node -r esm  app.js ",
    "dev-minsk": " cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "dev": " cross-env NODE_ENV=local_dev  concurrently \"npm run server\"  \"npm run client\"  ",
    "models": "sequelize-automate -c sequelize-automate.config.js",
    "testE": "tsc -sourcemap --module es6 --target esnext server/tests/entities/run.ts && mocha -r esm server/tests/entities/run.js",
    "testA": "tsc -sourcemap --module es6 --target esnext server/tests/analytic/run.ts && mocha -r esm server/tests/analytic/run.js"
  },
  "author": "Irina Puliak",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/config": "0.0.38",
    "@types/express": "^4.17.11",
    "@types/ini": "^1.3.30",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/sequelize": "^4.28.9",
    "chai": "^4.3.4",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "@types/validator": "^13.1.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.3.2",
    "connect-session-sequelize": "^7.1.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "ini": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "sequelize": "^6.3.5",
    "sequelize-automate": "^1.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "validator": "^13.5.2"
  }
}
